{
    "name": "adtech/authentication",
    "description": "",
    "keywords": [
        "adtech",
        "authentication"
    ],
    "homepage": "https://github.com/adtech/authentication",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Nguyen Cam",
            "email": "nguyenhuucam91@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Adtech\\Authentication\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adtech\\Authentication\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Adtech\\Authentication\\Providers\\AuthenticationServiceProvider"
            ]
        }
    }
}
